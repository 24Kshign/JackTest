<?xml version="1.0" encoding="utf-8"?>
<cn.share.jack.test.recyclerview.WrapRecyclerViewUIComponent xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id="@+id/lwr_fl_network_error"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone">

        <include
            android:id="@+id/lwr_net_work_error"
            layout="@layout/layout_network_error" />

    </FrameLayout>

    <android.support.v7.widget.RecyclerView
        android:id="@+id/lwr_recyclerview"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</cn.share.jack.test.recyclerview.WrapRecyclerViewUIComponent>